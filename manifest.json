  "protocol_handlers": [
    {
      "protocol": "geo",
      "url": "./index.html?coords=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "./index.html?file=%s",
      "accept": {
        "application/vnd.google-earth.kml+xml": [".kml"],
        "application/vnd.google-earth.kmz": [".kmz"]
      }
    }
  ],
  "share_target": {
    "action": "./index.html?share=true",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "kml_files",
          "accept": ["application/vnd.google-earth.kml+xml", ".kml", ".kmz"]
        }
      ]
    }
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "handle_links": "preferred",
  "related_applications": [],
